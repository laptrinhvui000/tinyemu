    \ end="]"
    \ contained

syn match rakuHexSequence display "\x\+" contained
syn region rakuHexSequence
    \ matchgroup=rakuEscape
    \ start="\["
    \ end="]"
    \ contained

syn match rakuOctSequence display "\o\+" contained
