syn match rakuRegexBlockCrap "[^{]*" nextgroup=rakuRegexBlock skipwhite skipempty transparent contained

syn region rakuRegexBlock
    \ matchgroup=rakuNormal
    \ start="{"
    \ end="}"
    \ contained
    \ contains=@rakuRegexen,@rakuVariables

