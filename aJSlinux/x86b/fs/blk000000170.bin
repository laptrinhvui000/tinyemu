syn region	freebasicBinaryError	start="&b[01]*[2-9a-zA-Z]" end="\W"
syn match	freebasicInteger		"\<\d\+\(u\=l\=\|lu\|f\)\>"
"
"	Floating point number, with dot, optional exponent
"
syn match	freebasicFloat		"\<\d\+\.\d*\(e[-+]\=\d\+\)\=[fl]\=\>"
