    \ matchgroup=rakuBracketComment
    \ start="#[`|=]<<<"
    \ skip="<<<\%([^>]\|>\%(>>\)\@!\)*>>>"
    \ end=">>>"
    \ contains=rakuAttention,rakuBracketComment

syn region rakuBracketComment
    \ matchgroup=rakuBracketComment
    \ start="#[`|=]Â«Â«"
    \ skip="Â«Â«\%([^Â»]\|Â»Â»\@!\)*Â»Â»"
    \ end="Â»Â»"
    \ contains=rakuAttention,rakuBracketComment
